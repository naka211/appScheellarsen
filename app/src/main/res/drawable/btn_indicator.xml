<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="18dip" />
            <stroke android:width="1dip" android:color="#d2b05f" />
            <gradient  android:angle="-90"  android:startColor="#d2b05f" android:endColor="#ac873f"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="18dip" />
            <stroke android:width="1dip" android:color="#ac873f" />
            <solid  android:color="#d2b05f"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="18dip" />
            <stroke android:width="1dip" android:color="#d2b05f" />
            <gradient  android:angle="-90"  android:startColor="#ac873f" android:endColor="#d2b05f" />
        </shape>
    </item>
</selector>